        var PanelAceptador_Billetes = $("#PanelAceptador_Billetes");
        var PanelAceptador_Monedas = $("#PanelAceptador_Monedas");
        var PanelDispensador = $("#PanelDispensador");
        var PanelImpresora = $("#PanelImpresora");
        var PanelMonitor = $("#PanelMonitor");
        var PanelScanner = $("#PanelScanner");
        var PanelTarjeta = $("#PanelTarjeta");
        var PanelToneleroA = $("#PanelToneleroA");
        var PanelToneleroB = $("#PanelToneleroB");

        var id01 = "ErrorCajero";
        var id02 = "StatusCajero";
        var id12 = "EfectivoActual";
        var id13 = "EfectivoDispensado";
        var id14 = "EfectivoInicial";

        var IdErrorAceptador_Billetes = "ErrorAceptador_Billetes";
        var IdStatusAceptador_Billetes = "StatusAceptador_Billetes";

        var IdErrorAceptador_Monedas = "ErrorAceptador_Monedas";
        var IdStatusAceptador_Monedas = "StatusAceptador_Monedas";

        var IdErrorDispensador = "ErrorDispensador";
        var IdStatusDispensador = "StatusDispensador";

        var IdStatusImpresora = "StatusImpresora";
        var IdErrorImpresora = "ErrorImpresora";
        var IdPapelImpresora = "PapelImpresora";

        var IdErrorScanner = "ErrorScanner";
        var IdStatusScanner = "StatusScanner";

        var IdErrorTarjeta = "ErrorTarjeta";
        var IdStatusTarjeta = "StatusTarjeta";

        var IdErrorToneleroA = "ErrorToneleroA";
        var IdStatusToneleroA = "StatusToneleroA";

        var IdErrorToneleroB = "ErrorToneleroB";
        var IdStatusToneleroB = "StatusToneleroB";

        var alertInfo = "class='alert alert-info'";
        var alertDanger = "class='alert alert-danger'";
        var alertSuccess = "class='alert alert-success'";
        var alertWarning = "class='alert alert-warning'";


        var pbInitial = $("#pbInitial");
        var pbDispensed = $("#pdDispensed");
        var pbUpdate = $("#pdUpdate");

        $(function() {

            var proxy = $.connection.atmMonitorHub;

            proxy.client.AtmData = function(jsonData) {

                Cajero = $.parseJSON(jsonData);

                $("#" + IdErrorToneleroB).replaceWith("<div id=" + IdErrorToneleroB + " " + alertInfo + "><strong>" + Cajero.ToneleroB.ErrorTonelero.substring(0, 12) + "</strong></div>");
                $("#" + IdStatusToneleroB).replaceWith("<div id=" + IdStatusToneleroB + " " + (Cajero.ToneleroB.StatusTonelero ? alertDanger : alertSuccess) + ")'><strong>" + (Cajero.ToneleroB.StatusTonelero ? "Error" : "Okey") + "</strong></div>");

                $("#" + IdErrorToneleroA).replaceWith("<div id=" + IdErrorToneleroA + " " + alertInfo + "><strong>" + Cajero.ToneleroA.ErrorTonelero.substring(0, 12) + "</strong></div>");
                $("#" + IdStatusToneleroA).replaceWith("<div id=" + IdStatusToneleroA + " " + (Cajero.ToneleroA.StatusTonelero ? alertDanger : alertSuccess) + ")'><strong>" + (Cajero.ToneleroA.StatusTonelero ? "Error" : "Okey") + "</strong></div>");

                $("#" + IdErrorScanner).replaceWith("<div id=" + IdErrorScanner + " " + alertInfo + "><strong>" + Cajero.Scanner.ErrorScanner.substring(0, 12) + "</strong></div>");
                $("#" + IdStatusScanner).replaceWith("<div id=" + IdStatusScanner + " " + (Cajero.Scanner.StatusScanner ? alertDanger : alertSuccess) + ")'><strong>" + (Cajero.Scanner.StatusScanner ? "Error" : "Okey") + "</strong></div>");

                $("#" + IdErrorDispensador).replaceWith("<div id=" + IdErrorDispensador + " " + alertInfo + "><strong>" + Cajero.Dispensador.ErrorDispensador.substring(0, 12) + "</strong></div>");
                $("#" + IdStatusDispensador).replaceWith("<div id=" + IdStatusDispensador + " " + (Cajero.Dispensador.StatusDispensador ? alertDanger : alertSuccess) + ")'><strong>" + (Cajero.Dispensador.StatusDispensador ? "Error" : "Okey") + "</strong></div>");

                $("#" + IdErrorAceptador_Billetes).replaceWith("<div id=" + IdErrorAceptador_Billetes + " " + alertInfo + " ><strong>" + Cajero.Aceptador_Billetes.ErrorAceptador_Billetes.substring(0, 12) + "</strong></div>");
                $("#" + IdStatusAceptador_Billetes).replaceWith("<div id=" + IdStatusAceptador_Billetes + " " + (Cajero.Aceptador_Billetes.StatusAceptador_Billetes ? alertDanger : alertSuccess) + " ><strong>" + (Cajero.Aceptador_Billetes.StatusAceptador_Billetes ? "Error" : "Okey") + "</strong></div>");

                $("#" + IdErrorAceptador_Monedas).replaceWith("<div id=" + IdErrorAceptador_Monedas + " " + alertInfo + " ><strong>" + Cajero.Aceptador_Monedas.ErrorAceptador_Monedas.substring(0, 12) + "</strong></div>");
                $("#" + IdStatusAceptador_Monedas).replaceWith("<div id=" + IdStatusAceptador_Monedas + " " + (Cajero.Aceptador_Monedas.StatusAceptador_Monedas ? alertDanger : alertSuccess) + " ><strong>" + (Cajero.Aceptador_Monedas.StatusAceptador_Monedas ? "Error" : "Okey") + "</strong></div>");

                $("#" + IdErrorImpresora).replaceWith("<div id=" + IdErrorImpresora + " " + alertInfo + "><strong>" + Cajero.Impresora.ErrorImpresora.substring(0, 12) + "</strong></div>");
                $("#" + IdStatusImpresora).replaceWith("<div id=" + IdStatusImpresora + " " + (Cajero.Impresora.StatusImpresora ? alertDanger : alertSuccess) + ")'><strong>" + (Cajero.Impresora.StatusImpresora ? "Error" : "Okey") + "</strong></div>");
                $("#" + IdPapelImpresora).replaceWith("<div id=" + IdPapelImpresora + " " + (!Cajero.Impresora.PapelImpresora ? alertWarning : alertSuccess) + ")'><strong>" + (!Cajero.Impresora.PapelImpresora ? "Papel Bajo" : "Okey") + "</strong></div>");

                $("#" + IdErrorTarjeta).replaceWith("<div id=" + IdErrorTarjeta + " " + alertInfo + "><strong>" + Cajero.Tarjeta.ErrorTarjeta.substring(0, 12) + "</strong></div>");
                $("#" + IdStatusTarjeta).replaceWith("<div id=" + IdStatusTarjeta + " " + (Cajero.Tarjeta.StatusTarjeta ? alertDanger : alertSuccess) + ")'><strong>" + (Cajero.Tarjeta.StatusTarjeta ? "Error" : "Okey") + "</strong></div>");


                var pa = 100 / (Cajero.EfectivoInicial / Cajero.EfectivoDispensado);
                var pb = 100 - pa;

                pbInitial.text(pa + "%");
                pbDispensed.text(pb + "%");
                pbInitial.css({ "width": pa + "%" });
                pbDispensed.css({ "width": pb + "%" });

                $("#" + id12).replaceWith("<p id=" + id12 + ">" + Cajero.EfectivoActual + "</p>");
                $("#" + id13).replaceWith("<p id=" + id13 + ">" + Cajero.EfectivoDispensado + "</p>");
                $("#" + id14).replaceWith("<p id=" + id14 + ">" + Cajero.EfectivoInicial + "</p>");

                BarChartDispensador.datasets[1].bars[0].value = Cajero.Dispensador.EfectivoInicialDispensador;
                BarChartDispensador.datasets[1].bars[1].value = Cajero.Dispensador.EfectivoDispensadoDispensador;
                BarChartDispensador.datasets[1].bars[2].value = Cajero.Dispensador.EfectivoActualDispensador;

                BarChartToneleroA.datasets[1].bars[0].value = Cajero.ToneleroA.EfectivoInicialTonelero;
                BarChartToneleroA.datasets[1].bars[1].value = Cajero.ToneleroA.EfectivoDispensadoTonelero;
                BarChartToneleroA.datasets[1].bars[2].value = Cajero.ToneleroA.EfectivoActualTonelero;

                BarChartToneleroB.datasets[1].bars[0].value = Cajero.ToneleroB.EfectivoInicialTonelero;
                BarChartToneleroB.datasets[1].bars[1].value = Cajero.ToneleroB.EfectivoDispensadoTonelero;
                BarChartToneleroB.datasets[1].bars[2].value = Cajero.ToneleroB.EfectivoActualTonelero;


                BarChartDispensador.update();
                BarChartToneleroA.update();
                BarChartToneleroB.update();

                Cajero.Dispensador.StatusDispensador ? PanelDispensador.switchClass("panel-info", "panel-danger") : PanelDispensador.switchClass("panel-danger", "panel-info");
                Cajero.ToneleroA.StatusTonelero ? PanelToneleroA.switchClass("panel-info", "panel-danger") : PanelToneleroA.switchClass("panel-danger", "panel-info");
                Cajero.ToneleroB.StatusTonelero ? PanelToneleroB.switchClass("panel-info", "panel-danger") : PanelToneleroB.switchClass("panel-danger", "panel-info");
                Cajero.Aceptador_Monedas.StatusAceptador_Monedas ? PanelAceptador_Monedas.switchClass("panel-info", "panel-danger") : PanelAceptador_Monedas.switchClass("panel-danger", "panel-info");
                Cajero.Aceptador_Billetes.StatusAceptador_Billetes ? PanelAceptador_Billetes.switchClass("panel-info", "panel-danger") : PanelAceptador_Billetes.switchClass("panel-danger", "panel-info");
                Cajero.Scanner.StatusScanner ? PanelScanner.switchClass("panel-info", "panel-danger") : PanelScanner.switchClass("panel-danger", "panel-info");
                Cajero.Tarjeta.StatusTarjeta ? PanelTarjeta.switchClass("panel-info", "panel-danger") : PanelTarjeta.switchClass("panel-danger", "panel-info");
                Cajero.Impresora.StatusImpresora ? PanelImpresora.switchClass("panel-info", "panel-danger") : PanelImpresora.switchClass("panel-danger", "panel-info");

            };

            proxy.client.notificationStatus = function(data) {
                if (data === "Enabled") {
                    $("#enabledNotificationId div").first().replaceWith("<div><input type='hidden' id='enabledid' value='False'/><div class='alert alert-info'>NOTIFICACIONES HABILITADAS. <a href='#' style='color: white'><strong>DESHABILITAR?</strong></a></div></div>");
                } else if (data === "Disabled") {
                    $("#enabledNotificationId div").first().replaceWith("<div><input type='hidden' id='enabledid' value='True'/><div class='alert alert-warning'>NOTIFICACIONES DESHABILITADAS. <a href='#' style='color: white'><strong>HABILITAR?</strong></a></div></div>");
                }
            }

            $.connection.hub.start().done(function() {
                proxy.server.registerNotification("@Model.IdCajero");
            });

            $(window).bind('beforeunload', function() {
                proxy.server.unRegisterNotification("@Model.IdCajero");
            });

            $("#enabledNotificationId").click(function() {
                proxy.server.enabledNotifications(@Model.IdCajero, $("#enabledid").val());
            });

        });

        function cashLevelsContent() {
            $.getJSON('@Url.Action("GetCashLevels")', { IdCajero: '@Model.IdCajero' }, function(data) {

                var pa = 100 / (data["EfectivoInicial"] / data["EfectivoDispensado"]);
                var pb = 100 - pa;

                pbInitial.text(pa + "%");
                pbDispensed.text(pb + "%");
                pbInitial.css({ "width": pa + "%" });
                pbDispensed.css({ "width": pb + "%" });

                $.each(data, function(key, val) {
                    $("#" + key).replaceWith("<p id=" + key + ">" + val + "</p>");
                });
            });
        }

        var chartOptions = {
            responsive: true,
            maintainAspectRatio: true,
            animationSteps: 30
        };

        var ChartDispensador = {
            labels: ["Inicial", "Dispensado", "Actual"],
            datasets: [
                {
                    data: [0, 0, 0],
                    fillColor: "rgba(0, 0, 0, 0)",
                    strokeColor: "rgba(0,0,0,0)",
                    highlightFill: "rgba(0,0,0,0)",
                    highlightStroke: "rgba(0,0,0,0)"
                }, {
                    data: [@Model.Dispensador.EfectivoInicialDispensador, @Model.Dispensador.EfectivoDispensadoDispensador, @Model.Dispensador.EfectivoActualDispensador],
                    fillColor: "rgba(0, 140, 186, 0.67)",
                    strokeColor: "rgba(220,220,220,0.8)",
                    highlightFill: "rgba(220,220,220,0.75)",
                    highlightStroke: "rgba(220,220,220,1)"
                }, {
                    data: [0, 0, 0],
                    fillColor: "rgba(0, 140, 186, 0)",
                    strokeColor: "rgba(0,0,0,0)",
                    highlightFill: "rgba(0,0,0,0)",
                    highlightStroke: "rgba(0,0,0,0)"
                }
            ]
        };

        var ChartToneleroA = {
            labels: ["Inicial", "Dispensado", "Actual"],
            datasets: [
                {
                    data: [0, 0, 0],
                    fillColor: "rgba(0, 0, 0, 0)",
                    strokeColor: "rgba(0,0,0,0)",
                    highlightFill: "rgba(0,0,0,0)",
                    highlightStroke: "rgba(0,0,0,0)"
                }, {
                    data: [@Model.ToneleroA.EfectivoInicialTonelero, @Model.ToneleroA.EfectivoDispensadoTonelero, @Model.ToneleroA.EfectivoActualTonelero],
                    fillColor: "rgba(0, 140, 186, 0.67)",
                    strokeColor: "rgba(220,220,220,0.8)",
                    highlightFill: "rgba(220,220,220,0.75)",
                    highlightStroke: "rgba(220,220,220,1)"
                }, {
                    data: [0, 0, 0],
                    fillColor: "rgba(0, 140, 186, 0)",
                    strokeColor: "rgba(0,0,0,0)",
                    highlightFill: "rgba(0,0,0,0)",
                    highlightStroke: "rgba(0,0,0,0)"
                }
            ]
        };

        var ChartToneleroB = {
            labels: ["Inicial", "Dispensado", "Actual"],
            datasets: [
                {
                    data: [0, 0, 0],
                    fillColor: "rgba(0, 0, 0, 0)",
                    strokeColor: "rgba(0,0,0,0)",
                    highlightFill: "rgba(0,0,0,0)",
                    highlightStroke: "rgba(0,0,0,0)"
                }, {
                    data: [@Model.ToneleroA.EfectivoInicialTonelero, @Model.ToneleroA.EfectivoDispensadoTonelero, @Model.ToneleroA.EfectivoActualTonelero],
                    fillColor: "rgba(0, 140, 186, 0.67)",
                    strokeColor: "rgba(220,220,220,0.8)",
                    highlightFill: "rgba(220,220,220,0.75)",
                    highlightStroke: "rgba(220,220,220,1)"
                }, {
                    data: [0, 0, 0],
                    fillColor: "rgba(0, 140, 186, 0)",
                    strokeColor: "rgba(0,0,0,0)",
                    highlightFill: "rgba(0,0,0,0)",
                    highlightStroke: "rgba(0,0,0,0)"
                }
            ]
        };

        var CtxDispensador = $("#Dispensador-canvas-chart").get(0).getContext("2d");
        var BarChartDispensador = new Chart(CtxDispensador).Bar(ChartDispensador, chartOptions);

        var CtxToneleroA = $("#ToneleroA-canvas-chart").get(0).getContext("2d");
        var BarChartToneleroA = new Chart(CtxToneleroA).Bar(ChartToneleroA, chartOptions);

        var CtxToneleroB = $("#ToneleroB-canvas-chart").get(0).getContext("2d");
        var BarChartToneleroB = new Chart(CtxToneleroB).Bar(ChartToneleroB, chartOptions);

        cashLevelsContent();


    </script>
}
