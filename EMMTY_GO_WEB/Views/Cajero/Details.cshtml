                Cajeros: null
            },
            ToneleroA: {
                IdTonelero: null,
                NombreTonelero: null,
                ModeloTonelero: null,
                STonelero: null,
                StatusTonelero: null,
                TipoMoneda: null,
                EfectivoActualTonelero: null,
                EfectivoDispensadoTonelero: null,
                EfectivoInicialTonelero: null,
                ErrorTonelero: null,
                arcaTonelero: null,
                Cajeros: null,
                Cajeros1: null
            },
            ToneleroB: {
                IdTonelero: null,
                NombreTonelero: null,
                ModeloTonelero: null,
                NSTonelero: null,
                StatusTonelero: null,
                TipoMoneda: null,
                EfectivoActualTonelero: null,
                EfectivoDispensadoTonelero: null,
                EfectivoInicialTonelero: null,
